{
  "name": "share-data-extension",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:extension_dev": "webpack --config extension.config.dev.js",
    "build:extension_prod": "webpack --config extension.config.prod.js",
    "build:all": "npm run build && npm run build:extension_dev",
    "preview": "vite preview"
  },
  "dependencies": {
    "file-loader": "^6.2.0",
    "json-loader": "^0.5.7",
    "ts-loader": "^9.3.0",
    "vue": "^3.2.25",
    "webpack": "^5.72.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "copy-webpack-plugin": "^10.2.4",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vue-tsc": "^0.34.7",
    "webpack-cli": "^4.9.2"
  }
}
